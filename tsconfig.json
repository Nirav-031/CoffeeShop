{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@navigation/*": ["navigation/*"],
      "@utils/*": ["utils/*"],
      "@store/*": ["store/*"],
      "@data/*": ["data/*"],
    },
    "jsx": "react-native",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js","@react-native/typescript-config/tsconfig.json"]
}
